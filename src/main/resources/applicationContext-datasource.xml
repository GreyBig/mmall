<li class="L46" rel="L46">         &lt;!--&lt;property name=&#34;removeAbandoned&#34; value=&#34;true&#34; /&gt;--&gt;</li>
<li class="L47" rel="L47">        &lt;!-- 数据库连接过多长时间不用将被视为被遗弃而收回连接池中 --&gt;</li>
<li class="L48" rel="L48">         &lt;!--&lt;property name=&#34;removeAbandonedTimeout&#34; value=&#34;120&#34; /&gt;--&gt;</li>
<li class="L49" rel="L49">        &lt;!-- #连接的超时时间，默认为半小时。 --&gt;</li>
<li class="L50" rel="L50">        &lt;property name=&#34;minEvictableIdleTimeMillis&#34; value=&#34;${db.minEvictableIdleTimeMillis}&#34;/&gt;</li>
<li class="L51" rel="L51"></li>
<li class="L52" rel="L52">        &lt;!--# 失效检查线程运行时间间隔，要小于MySQL默认--&gt;</li>
<li class="L53" rel="L53">        &lt;property name=&#34;timeBetweenEvictionRunsMillis&#34; value=&#34;40000&#34;/&gt;</li>
<li class="L54" rel="L54">        &lt;!--# 检查连接是否有效--&gt;</li>
<li class="L55" rel="L55">        &lt;property name=&#34;testWhileIdle&#34; value=&#34;true&#34;/&gt;</li>
<li class="L56" rel="L56">        &lt;!--# 检查连接有效性的SQL语句--&gt;</li>
<li class="L57" rel="L57">        &lt;property name=&#34;validationQuery&#34; value=&#34;SELECT 1 FROM dual&#34;/&gt;</li>
<li class="L58" rel="L58">    &lt;/bean&gt;</li>
<li class="L59" rel="L59"></li>
<li class="L60" rel="L60">    &lt;bean id=&#34;sqlSessionFactory&#34; class=&#34;org.mybatis.spring.SqlSessionFactoryBean&#34;&gt;</li>
<li class="L61" rel="L61">        &lt;property name=&#34;dataSource&#34; ref=&#34;dataSource&#34;/&gt;</li>
<li class="L62" rel="L62">        &lt;property name=&#34;mapperLocations&#34; value=&#34;classpath*:mappers/*Mapper.xml&#34;&gt;&lt;/property&gt;</li>
<li class="L63" rel="L63"></li>
<li class="L64" rel="L64">        &lt;!-- 分页插件 --&gt;</li>
<li class="L65" rel="L65">        &lt;property name=&#34;plugins&#34;&gt;</li>
<li class="L66" rel="L66">            &lt;array&gt;</li>
<li class="L67" rel="L67">                &lt;bean class=&#34;com.github.pagehelper.PageHelper&#34;&gt;</li>
<li class="L68" rel="L68">                    &lt;property name=&#34;properties&#34;&gt;</li>
<li class="L69" rel="L69">                        &lt;value&gt;</li>
<li class="L70" rel="L70">                            dialect=mysql</li>
<li class="L71" rel="L71">                        &lt;/value&gt;</li>
<li class="L72" rel="L72">                    &lt;/property&gt;</li>
<li class="L73" rel="L73">                &lt;/bean&gt;</li>
<li class="L74" rel="L74">            &lt;/array&gt;</li>
<li class="L75" rel="L75">        &lt;/property&gt;</li>
<li class="L76" rel="L76"></li>
<li class="L77" rel="L77">    &lt;/bean&gt;</li>
<li class="L78" rel="L78"></li>
<li class="L79" rel="L79">    &lt;bean name=&#34;mapperScannerConfigurer&#34; class=&#34;org.mybatis.spring.mapper.MapperScannerConfigurer&#34;&gt;</li>
<li class="L80" rel="L80">        &lt;property name=&#34;basePackage&#34; value=&#34;com.mmall.dao&#34;/&gt;</li>
<li class="L81" rel="L81">    &lt;/bean&gt;</li>
<li class="L82" rel="L82"></li>
<li class="L83" rel="L83">    &lt;!-- 使用@Transactional进行声明式事务管理需要声明下面这行 --&gt;</li>
<li class="L84" rel="L84">    &lt;tx:annotation-driven transaction-manager=&#34;transactionManager&#34; proxy-target-class=&#34;true&#34; /&gt;</li>
<li class="L85" rel="L85">    &lt;!-- 事务管理 --&gt;</li>
<li class="L86" rel="L86">    &lt;bean id=&#34;transactionManager&#34; class=&#34;org.springframework.jdbc.datasource.DataSourceTransactionManager&#34;&gt;</li>
